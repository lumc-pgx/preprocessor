rule laa_summary:
    """
    Summarize LAA results
    """
    input:
        expand("preprocessor/LAA/{barcodes}_summary.csv", barcodes=PARAMS.barcode_ids)
    output:
        "preprocessor/summary/LAA/laa_summary.csv"
    shell:
        "head -n1 {input[0]} > {output} && "
        "cat {input} | grep -v 'BarcodeName,FastaName' >> {output}"
